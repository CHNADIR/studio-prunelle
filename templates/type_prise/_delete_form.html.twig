<form method="post" action="{{ path('app_type_prise_delete', {'id': type_prise.id}) }}" onsubmit="return confirm('Êtes-vous sûr de vouloir supprimer ce type de prise ?');" class="d-inline">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ type_prise.id) }}">
    <button type="submit" class="btn btn-danger">
        <i class="bi bi-trash"></i> Supprimer
    </button>
</form>
